server {
    listen       80 proxy_protocol;
    set_real_ip_from dev-iru-swarm1.infoclinica.lan;
    set_real_ip_from dev-iru-swarm2.infoclinica.lan;
    set_real_ip_from dev-iru-swarm3.infoclinica.lan;
    real_ip_header proxy_protocol;
    server_name  proxyprotocol;
    

    location / {
        root   /usr/share/nginx/html;
        index  index.html index.htm;
    }
    error_page   500 502 503 504  /50x.html;
    location = /50x.html {
        root   /usr/share/nginx/html;
    }
}
